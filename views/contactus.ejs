<%- include('partials/header') %>
<div class="row">
  <div class="col-sm">
       <!-- <h1 class="mt-4 ">Welcome!</h1> -->
  </div>

 <div class="col-sm">

  <form action="/contactUs" method="POST">
    <h1 class="mt-4">Contact Us</h1>
  <div class="form-group">
  <label for="fullname">Fullname</label>
  <input type="text" tabindex="1" name="fullname" class="form-control" id="fullname" placeholder="Enter fullname" required>
</div>

<div class="form-group">
  <label for="exampleInputEmail1">Email address</label>
  <input type="email" tabindex="2" name="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email" required>
</div>

<div class="form-group">
  <label for="message">Message</label>
  <textarea rows="5" tabindex="3" name="message" class="form-control" id="message" placeholder="Type message.." required></textarea>
</div>
<button type="submit" tabindex="4" class="btn btn-primary">Submit</button>
</form>

 </div>        
 
 <div class="col-sm"> 
   
 </div>
 
</div>


   
        
  
